{
    "name": "fof/redis",
    "description": "Adds Redis cache, sessions and queue to Flarum",
    "keywords": [
        "queue", "cache", "session", "redis"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "FriendsOfFlarum",
            "homepage": "https://friendsofflarum.org"
        }
    ],
    "support": {
        "issues": "https://github.com/FriendsOfFlarum/redis/issues",
        "source": "https://github.com/FriendsOfFlarum/redis",
        "forum": "https://discuss.flarum.org/d/21873"
    },
    "homepage": "https://friendsofflarum.org",
    "funding": [
        {
            "type": "website",
            "url": "https://opencollective.com/fof/donate"
        }
    ],
    "extra": {
        "branch-alias": {
            "2.x": "2.x-dev"
        }
    },
    "require": {
        "flarum/core": "2.x-dev",
        "illuminate/redis": "^11.42",
        "predis/predis": "^v2.3.0"
    },
    "autoload": {
        "psr-4": {
            "FoF\\Redis\\": "src/"
        }
    },
    "require-dev": {
        "flarum/phpstan": "2.x-dev"
    },
    "scripts": {
        "analyse:phpstan": "phpstan analyse",
        "clear-cache:phpstan": "phpstan clear-result-cache"
    },
    "scripts-descriptions": {
        "analyse:phpstan": "Run static analysis"
    },
    "minimum-stability": "dev"
}
